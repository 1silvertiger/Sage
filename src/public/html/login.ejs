<html lang="en">

<head>
  <%- include('./head.html') %>

  <!-- Custom styles -->
  <link rel="stylesheet" type="text/css" href="stylesheets/css/login.css">

  <!-- Custom scripts -->
  <script src="scripts/login.ejs.js" type="text/javascript" defer></script>
</head>

<body class="text-center">

  <div class="container">
    <div class="row">
      <div class="col s12 m12 l12">
        <form class="form-signin">
          <img class="mb-4" src="" alt="Logo" width="72" height="72">
          <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
          <div class="g-signin2" data-onsuccess="onSignIn" data-theme="dark"></div>
          <p class="mt-5 mb-3 text-muted">&copy; 2019</p>
        </form>
        <div class="preloader-wrapper big">
          <div class="spinner-layer spinner-green-only">
            <div class="circle-clipper left">
              <div class="circle"></div>
            </div>
            <div class="gap-patch">
              <div class="circle"></div>
            </div>
            <div class="circle-clipper right">
              <div class="circle"></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

</body>

<script>
  const URL = '<%= URL %>';
  const returnPath = '<%= returnPath %>';
  const PLAID_ENV = '<%= PLAID_ENV %>'
</script>

</html>