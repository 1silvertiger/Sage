<!doctype html>
<html>
<%- include ./head.html %>

<body>
<%- include ./navbar.html %>

    <div class="container">
        <div class="row">
            <table id="table" class="striped">
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Official name</th>
                        <th>Type</th>
                        <th>Subtype</th>
                        <th>Mask</th>
                    </tr>
                </thead>
                <tbody>
                    <tr v-for="account in accounts">
                        <td>{{account.name}}</td>
                        <td>{{account.official_name}}</td>
                        <td>{{account.type}}</td>
                        <td>{{account.subtype}}</td>
                        <td>{{account.mask}}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</body>

<script>
    const accounts = JSON.parse('<%- JSON.stringify(accounts) %>');
</script>
<script src="scripts/accounts.ejs.js" defer></script>
</html>